<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nailedit.project.nailedit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
                android:label="@string/activity_customer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable">
            </meta-data>
        </activity>

        <activity android:name=".TableMapActivity"
                android:label="@string/activity_tablet">
        </activity>

        <service
            android:enabled="true"
            android:name="com.nailedit.project.nailedit.ServiceBackground.MyService" >
        </service>

        <receiver
            android:name="com.nailedit.project.nailedit.ServiceBackground.MyReceiver"
            android:exported="false" >
            <intent-filter >
                <action android:name="com.nailedit.project.nailedit.Service.Receiver"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>